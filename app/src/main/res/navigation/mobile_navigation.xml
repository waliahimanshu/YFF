<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.himanshuw.yff.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_add_record"
        android:name="com.himanshuw.yff.ui.addaccounts.AddAccountsFragment"
        android:label="@string/title_add_entry"
        tools:layout="@layout/fragment_all_accounts" />

    <fragment
        android:id="@+id/navigation_read_more"
        android:name="com.himanshuw.yff.ui.more.MoreInfoFragment"
        android:label="@string/title_read_more"
